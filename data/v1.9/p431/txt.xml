        <head xml:id="id080dadcc-edd3-4f21-b716-4afdc936093e" type="topText" n="»I Adresseavisen«"/>
        <dateline xml:id="idbfb12b2e-9bd8-4e30-ad5d-4bd9ccd0e022">
          <date xml:id="id626bc1da-8465-4df3-abd0-bc36c0fc1684" when="18510900"/>
        </dateline>
