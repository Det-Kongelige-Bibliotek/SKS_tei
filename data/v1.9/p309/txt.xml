      <div type="entry" n="310" xml:id="n310">
        <head xml:id="iddcf63b05-f14a-4aee-a29b-485c63f184ca" type="topText" n="»Smaating fra Aarene nogle og 40«"/>
        <dateline xml:id="id027ab47b-1227-4ee2-89a9-aff83ff2ac23">
          <date xml:id="idd2d90e54-ee6e-4f69-9813-9be1e7ebbc2c" when="18440000"/>
