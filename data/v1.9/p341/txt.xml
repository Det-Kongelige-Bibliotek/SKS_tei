        <div xml:id="id06c8b9e4-8cbb-4693-9f78-493b3caace27" type="mainColumn" rend="full">
          <p xml:id="id8a3d8628-b17e-4981-8392-4e2e7524a898"><milestone xml:id="id3f93dc94-ebe7-4251-867d-63166de94682" unit="entry" edRef="#Pap." n="VII1B210"/><pb xml:id="s5r" n="5r" rend="supplied"/><ref type="commentary" target="kom.xml#p-6355" xml:id="p-6355"><choice xml:id="id2d1d231f-a433-4560-b74e-4d9b53feabaa"><abbr xml:id="idddce9b3e-8355-455a-9da6-35b16f4c5f8d">N<hi xml:id="idb076c702-5515-409c-ab5c-0ff0ab08c4bf" rendition="#sup">o</hi></abbr><expan xml:id="id4f1c979f-1874-4a95-b3b4-74a821f02f7d" ana="#means17">Numero</expan><expan rend="means" xml:id="means17">nummer</expan></choice> 5</ref>.</p>
          <p xml:id="id613af2df-a4ff-46fa-b187-3ee2f086d757" rend="decoration">
            <figure xml:id="id417466f7-fd30-4356-a501-03dbb37c16a0" type="blank"/>
          </p>
          <p xml:id="idd1156683-ea78-42c7-887d-53fb6561b41c" rendition="#xlang">
            <hi xml:id="id4949b79b-c974-440a-91f8-1515e41b1abf" rendition="#und">Efteraaret er</hi>
            <hi xml:id="idbc0f4bee-da4c-433e-ae77-72c6ea6e4aa2" rendition="#dun">Erindringernes</hi>
            <hi xml:id="id9aed2f54-f3a4-49cb-8133-0ef768afa79a" rendition="#und">Tid.</hi>
          </p>
          <p xml:id="idd8cb0e1a-3f84-4baa-b171-a3543e44530b" rend="decoration">
            <figure xml:id="id28b99f1b-0f0c-42b3-88af-723f173d06ea" type="blank"/>
          </p>
          <table xml:id="id4cf549b3-63a9-4466-b07b-790fc82d0bba">
