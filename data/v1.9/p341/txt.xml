            <ref type="commentary" target="kom.xml#p-6336" xml:id="p-6336"><choice xml:id="id06dffec0-c0d8-48f8-be96-dfa120716f84"><abbr xml:id="id77ef6df3-e734-42b2-a787-f1b25de52bc8">cfr</abbr><expan xml:id="id68752e31-ec71-4eaf-9b04-3c215cb7d9e3" ana="#means1">confer</expan><expan rend="means" xml:id="means1">jævnfør</expan></choice> Journalen JJ <choice xml:id="id007948a7-ddca-41bd-9703-ab8a5f8d4062"><abbr xml:id="id7245e042-ff5f-493e-88d3-87e97af96c6f">p.</abbr><expan xml:id="id0b09f090-d004-4476-ae21-5fdde55576e3" ana="#means2">pagina</expan><expan rend="means" xml:id="means2">side</expan></choice>
